   nuevo_array = []
def nuevolistarEncomiendas(self):
        print("\nEncomiendas (Nuevo Array): \n")
        for con in self.nuevo_array:
            datos = "Direccion: {0}, Peso: {1}kg, Origen: {2}, Medidas: {3}x{4}x{5}"
            print(datos.format(con.direccion, con.peso, con.c_origen, con.longitud, con.ancho, con.alto))
        print(" ")
# objeto Courier
miCourier = Courier()
miCourier.nuevolistarEncomiendas() #para listar 
